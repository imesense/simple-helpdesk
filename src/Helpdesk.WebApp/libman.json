{
    "$schema": "https://json.schemastore.org/libman.json",
    "version": "1.0",
    "defaultProvider": "cdnjs",
    "libraries": [
        {
            "library": "bootstrap@5.1.0",
            "destination": "wwwroot/lib/bootstrap/",
            "files": [
                "js/bootstrap.js",
                "js/bootstrap.min.js",
                "css/bootstrap-grid.css",
                "css/bootstrap-grid.min.css",
                "css/bootstrap-reboot.css",
                "css/bootstrap-reboot.min.css",
                "css/bootstrap-utilities.css",
                "css/bootstrap-utilities.min.css",
                "css/bootstrap.css",
                "css/bootstrap.min.css"
            ]
        },
        {
            "library": "jquery@3.6.0",
            "destination": "wwwroot/lib/jquery/",
            "files": [
                "jquery.js",
                "jquery.min.js"
            ]
        },
        {
            "provider": "unpkg",
            "library": "jquery-validation@1.17.0",
            "destination": "wwwroot/lib/jquery-validation/",
            "files": [
                "dist/additional-methods.js",
                "dist/jquery.validate.js",
                "dist/jquery.validate.min.js"
            ]
        },
        {
            "library": "jquery-validation-unobtrusive@4.0.0",
            "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
            "files": [
                "jquery.validate.unobtrusive.js",
                "jquery.validate.unobtrusive.min.js"
            ]
        },
        {
            "library": "font-awesome@6.2.1",
            "destination": "wwwroot/lib/font-awesome/",
            "files": [
                "css/all.css",
                "css/all.min.css",
                "css/fontawesome.css",
                "css/fontawesome.min.css",
                "css/solid.css",
                "css/solid.min.css",
                "webfonts/fa-regular-400.ttf",
                "webfonts/fa-regular-400.woff2",
                "webfonts/fa-solid-900.ttf",
                "webfonts/fa-solid-900.woff2"
            ]
        }
    ]
}
